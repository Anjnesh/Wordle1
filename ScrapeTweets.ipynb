   "source": [
    "def is_valid_wordle_tweet(tweet, wordle_id): \n",
    "    text = (tweet.replace(\"Y\", \"y\").replace(\"游릴\", \"Y\")\n",
    "                 .replace(\"M\", \"m\").replace(\"游릳\", \"M\")\n",
    "                 .replace(\"N\", \"n\").replace(\"拘뜦", \"N\").replace(\"拘\", \"N\"))\n",
    "    \n",
    "    for i in range(wordle_id-20, wordle_id+20):\n",
    "        if i==wordle_id:\n",
    "            continue\n",
    "        if str(i) in text:\n",
    "            return False\n",
    "    \n",
    "    if len(re.findall(\"Wordle %d\" % wordle_id, text)) != 1:\n",
    "        return False\n",
    "\n",
    "    if re.match(\"Wordle %d [2-6]/6\\n\\n[YMN]{5}\\n\" % wordle_id, text) is None:\n",
    "        return False\n",
    "    return True     "
   ]
  },
